@font-face
  font-family 'Roboto'
  src url('app/fonts/Roboto-Regular.eot')
  src url('app/fonts/Roboto-Regular.eot?#iefix') format('embedded-opentype')
      url('app/fonts/Roboto-Regular.woff') format('woff')
      url('app/fonts/Roboto-Regular.ttf') format('truetype')
  font-weight normal
  font-style normal

@font-face
  font-family 'Roboto'
  src url('app/fonts/Roboto-Medium.eot')
  src url('app/fonts/Roboto-Medium.eot?#iefix') format('embedded-opentype')
      url('app/fonts/Roboto-Medium.woff') format('woff')
      url('app/fonts/Roboto-Medium.ttf') format('truetype')
  font-weight 500
  font-style normal